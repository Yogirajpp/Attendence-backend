# Server Configuration
NODE_ENV=development
PORT=3003

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/college-service

# JWT Configuration (same as Auth service)
JWT_SECRET=your-secret-key-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=*

# File Uploads
COLLEGE_LOGO_PATH=uploads/college-logos
COLLEGE_LOGO_MAX_SIZE=2097152